# GPUStack UI Configuration
# Development environment - for production, copy .env.production.template to .env.prod

# Environment mode (development, production, testing)
ENV=development

# GPUStack API Configuration (REQUIRED)
# Replace with your actual GPUStack server details
GPUSTACK_API_BASE=http://localhost:80
# GPUSTACK_API_TOKEN=your_gpustack_token_here

# Tavily Search API (OPTIONAL)
# TAVILY_API_KEY=your_tavily_api_key_here

# JWT Security (CHANGE IN PRODUCTION)
JWT_SECRET_KEY=dev-secret-key-not-for-production
JWT_ALGORITHM=HS256

# Backend Configuration
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8001

# Security - Development CORS (wide open for dev)
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8001", "http://127.0.0.1:3000"]
ALLOWED_HOSTS=["localhost", "127.0.0.1"]

